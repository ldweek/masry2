<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="installation_done_id" model="ir.cron">
        <field name="name">Check Installation Done</field>
        <field name="model_id" ref="model_sale_order_line"/>
        <field name="state">code</field>
        <field name="code">model.installation_is_done()</field>
        <field name='interval_number'>1</field>
